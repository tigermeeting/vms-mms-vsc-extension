{
  "name": "vms-mms",
  "displayName": "VMS MMS",
  "description": "Syntax highlighting for Makefile Macro Syntax (MMS) language",
  "version": "1.0.1",
  "publisher": "Tigermeeting",
  "icon": "images/mms.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/tigermeeting/vms-mms-vsc-extension.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "mms",
    "openvms",
    "vms"
  ],
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "mms",
        "aliases": [
          "MMS",
          "mms"
        ],
        "extensions": [
          ".mms"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mms",
        "scopeName": "source.mms",
        "path": "./syntaxes/mms.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "mms",
        "path": "./mms-snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "vscode:publish": "vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "web-streams-polyfill": "^4.0.0"
  }
}
